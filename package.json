{
  "name": "basic-microservice",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=staging nodemon ./bin/www",
    "dev": "NODE_ENV=development nodemon ./bin/www",
    "start-prod": "NODE_ENV=production node ./bin/www",
    "start-qa": "NODE_ENV=qa node ./bin/www",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write **/*.js && eslint . --fix",
    "db:sync": "NODE_ENV=development node scripts/sync-database.js",
    "db:sync:alter": "NODE_ENV=development node scripts/sync-database.js --alter",
    "db:sync:force": "NODE_ENV=development node scripts/sync-database.js --force",
    "db:sync:fresh": "NODE_ENV=development node scripts/sync-database.js --force --seed -y",
    "db:seed": "NODE_ENV=development node scripts/seed.js",
    "db:help": "NODE_ENV=development node scripts/sync-database.js --help",
    "commit": "cz",
    "prepare": "husky install"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.6.0",
    "apple-auth": "^1.0.7",
    "aws-sdk": "^2.1258.0",
    "axios": "^0.21.4",
    "bcryptjs": "^2.4.3",
    "bree": "^7.2.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cuid": "^3.0.0",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-fileupload": "^1.5.0",
    "express-ipfilter": "^1.3.1",
    "faker": "^5.5.3",
    "firebase-dynamic-links": "^1.1.0",
    "form-data": "^4.0.0",
    "google-auth-library": "^7.11.0",
    "http-status": "^1.5.0",
    "image-size": "^1.0.2",
    "joi": "^17.4.0",
    "joi-phone-number": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.12",
    "oracledb": "^6.3.0",
    "pdfkit": "^0.17.2",
    "sequelize": "^6.37.0",
    "twilio": "^3.71.1",
    "xlsx": "^0.17.0"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.0.1",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "simple-git-hooks": "^2.2.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
